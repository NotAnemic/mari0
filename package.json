{
  "name": "tannerrogalsky.github.com",
  "version": "2.8.0",
  "description": "Tanner Rogalsky's personal website.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "npm run lint",
    "lint": "eslint src scripts",
    "build": "npm run clean && webpack --color --progress",
    "build:production": "npm run clean && webpack --color --progress --config=webpack.config.production.babel.js",
    "deploy:setup": "babel-node scripts/setupDeploy.js",
    "deploy:push": "babel-node scripts/pushDeploy.js",
    "start": "webpack-dev-server --port=8081",
    "preversion": "npm run clean && npm run test",
    "version": "npm run build:production && npm run deploy:setup",
    "postversion": "git push && git push --tags && npm run deploy:push && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TannerRogalsky/tannerrogalsky.github.com.git"
  },
  "author": "Tanner Rogalsky",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta8",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.15.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.26.5",
    "highlight.js": "^9.1.0",
    "html-loader": "^0.4.0",
    "make-stateful": "^1.1.0",
    "markdown-loader": "^0.1.7",
    "marked": "^0.3.5",
    "raf-runner": "^1.0.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "simple-git": "^1.21.0",
    "sitemap": "^1.5.0",
    "static-site-generator-webpack-plugin": "^2.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^2.0.6-beta",
    "webpack-dev-server": "^1.14.1"
  }
}
